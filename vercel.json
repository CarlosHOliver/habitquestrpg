{
  "rewrites": [
    {
      "source": "/",
      "destination": "/auth-check.html"
    },
    {
      "source": "/login",
      "destination": "/login-standalone.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/settings",
      "destination": "/settings.html"
    },
    {
      "source": "/achievements",
      "destination": "/achievements.html"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
